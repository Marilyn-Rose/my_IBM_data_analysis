    "tags": []
   },
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Upper bounds:209698.0\n",
      "Lower bounds:-82830.0\n"
     ]
    }
   ],
   "source": [
    "Upper_bounds = Q3 + IQR * 1.5\n",
    "Lower_bounds = Q1 - IQR * 1.5\n",
    "\n",
    "print(f\"Upper bounds:{Upper_bounds}\")\n",
    "print(f\"Lower bounds:{Lower_bounds}\")"
   ]
  },
  {
